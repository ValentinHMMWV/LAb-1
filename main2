#include <iostream>
using namespace std;
#include<cmath>
int main()
{
	//Begin 16.Знайти значення функції y = 3x6-6x2- 7 при даному значенні x.
	//декларація змінної
	float x, y;
	cout << "\n_____Begin 16_____" << endl;

	//ввод змінної
	cout << "Enter x=";
	cin >> x;

	//розрахунок результату
	y = 3 * pow(x, 6) - 6 * pow(x, 2) - 7;

	//вивід результату
	cout << "function value y="<<y<<endl;
	 
	return 0;

}
